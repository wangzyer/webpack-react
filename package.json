  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -d --env.dev --config config/webpack.dev.js",
    "dev": "npm run start",
    "lib": "rimraf lib &&  webpack -p --env.config production --env.prod --config config/webpack.dll.js",
    "build": "rimraf dist && webpack -p --env.config production --config config/webpack.prod.js",
    "build:prod": "rimraf dist && webpack -p --env.config production --env.prod --config config/webpack.prod.js && npm run uploadTest && node ./build/index.js",
    "upload": "npm run build  && npm run uploadTest",
    "uploadTest": " upload --name react-webpack && del react-webpack.zip"
  },
  "license": "ISC",
  "devDependencies": {
